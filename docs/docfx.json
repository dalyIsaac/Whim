{
  "metadata": [{}],
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"],
        "exclude": ["_site/**", "obj/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern", "template"],
    "globalMetadata": {
      "_appName": "Whim",
      "_appTitle": "Whim",
      "_appFooter": "Copyright Â© 2021-2024 Isaac Daly",
      "_appFaviconPath": "images/icon.ico",
      "_appLogoPath": "images/icon.svg",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
